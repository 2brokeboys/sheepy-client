{
  "name": "voting-calculator-frontend",
  "version": "2.0.1",
  "date": "18.02.2018",
  "main": "application.js",
  "scripts": {
    "serve:website": "webpack-dev-server --inline --content-base ./src --watch-poll --host localhost --config webpack.config.website.js",
    "build:website": "rimraf ./dist/webpack/website && webpack --config webpack.config.website.js",
    "start:website": "http-server ./dist/webpack/website",
    "build:desktop": "rimraf ./dist/webpack/desktop && webpack --config webpack.config.electron-main.js && webpack --config webpack.config.electron-renderer.js",
    "start:desktop": "electron dist/webpack/desktop/application.js",
    "package:desktop": "npm run build:desktop && rimraf ./dist/packages && cp -f package.json ./dist/webpack/desktop/package.json && electron-packager ./dist/webpack/desktop Seminarwahlsystem --all --tmpdir=./dist/temp --out=./dist/packages --overwrite",
    "lint": "./node_modules/.bin/tslint -c tslint.json './src/**/*.ts'"
  },
  "author": "Paul Thiel",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "core-js": "^2.6.5",
    "fs": "^0.0.1-security",
    "http-server": "^0.11.1",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "^11.9.5",
    "angular2-template-loader": "^0.6.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "raw-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
